{
    "name": "mattmezza/socialize",
    "description": "A composer package to create a social network with ease.",
    "license": "MIT Licence",
    "authors": [
        {
            "name": "Matteo Merola",
            "email": "mattmezza@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "slim/slim": "^2.6",
        "neoxygen/neoclient": "^3.3",
        "symfony/yaml": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Socialize\\Account\\": "account/",
            "Socialize\\Feeds\\": "feeds/",
            "Socialize\\Follow\\": "follow/",
            "Socialize\\Friends\\": "friends/",
            "Socialize\\Posts\\": "posts/",
            "Socialize\\Profile\\": "profile/",
            "Socialize\\Users\\": "users/"
        }
    }
}
